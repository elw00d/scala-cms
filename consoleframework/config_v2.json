{
    "templates": [
        {
            "id": "master-page",
            "view": "master-page.ftl",
            "attributes": {
                "active-page": null
            }
        },
        {
            "id": "getting-started",
            "baseTemplate": "master-page",
            "regions": {
                "main": "getting-started.ftl"
            },
            "attributes": {
                "active-page": "docs"
            }
        },
        {
            "id": "downloads",
            "baseTemplate": "master-page",
            "regions": {
                "main": "downloads.ftl"
            },
            "attributes": {
                "active-page": "downloads"
            }
        }
    ],
    "modules": {
        "menuModule": {
            "className": "ru.MenuModule",
            "attributes": {
                "view": "menu.ftl"
            }
        }
    },
    "rootNode": {
        /* первый и последний слеш будет проигнорирован */
        "urlPrefix": "",
        "template": "master-page",
        "nodes": [
            {
                "urlPrefix": "getting-started.html",
                "template": "getting-started"
            },
            {
                "urlPrefix": "downloads.html",
                "template": "downloads"
            }
        ]
    }
}